{
  "name": "server-kit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "contributors": [
    "Brice Govin <brice.govin@ensta-bretagne.org>"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "buildDependencies": "node buildDependencies.js",
    "compile": "yarn clean && ./node_modules/.bin/tsc -p . && yarn buildDependencies",
    "test": "jest",
    "postinstall": "yarn compile",
    "package": "yarn compile && yarn buildDependencies"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "tests/(.*).test.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.17.2",
    "fs-extra": "^10.0.0",
    "lowdb": "^3.0.0",
    "querystring": "^0.2.0",
    "wait-on": "^4.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^24.0.11",
    "@types/node": "^17.0.5",
    "awesome-typescript-loader": "^5.2.1",
    "fetch": "^1.1.0",
    "fs": "^0.0.1-security",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "node-fetch": "^2.3.0",
    "ts-jest": "^24.0.1",
    "typescript": "^4.5.4"
  }
}
